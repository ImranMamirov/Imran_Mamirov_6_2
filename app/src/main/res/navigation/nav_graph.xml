<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph.xml"
    app:startDestination="@id/cartoonFragment">

    <fragment
        android:id="@+id/cartoonFragment"
        android:name="com.example.imran_mamirov_6_2.ui.fragments.cartoon.CartoonFragment"
        android:label="fragment_cartoon"
        tools:layout="@layout/fragment_cartoon">
        <action
            android:id="@+id/action_cartoonFragment_to_detailFragment"
            app:destination="@id/detailFragment" />
    </fragment>
    <fragment
        android:id="@+id/detailFragment"
        android:name="com.example.imran_mamirov_6_2.ui.fragments.detail.DetailFragment"
        android:label="DetailFragment">
        <argument
            android:name="id"
            android:defaultValue="-1"
            app:argType="integer" />
    </fragment>
</navigation>